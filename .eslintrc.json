{
  "extends": ["@antfu", "prettier"],
  "plugins": ["prettier"],
  "ignorePatterns": [
    "node_modules",
    "dist",
    "test",
    "jest.config.js",
    "interface.ts"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "curly": "off",
    "no-console": "off"
  }
}
